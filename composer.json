{
    "name": "jienckebd/bryanjiencke",
    "description": "Personal profile system",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
        "composer/installers": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.0",
        "goalgorilla/open_social": "^1.0",
        "doctrine/cache": "1.6.1",
        "doctrine/collections": "1.4.0",
        "doctrine/common": "2.7.2",
        "doctrine/inflector": "1.1.0",
        "drupal/developer_suite": "^1.2",
        "drupal/panelizer": "^4.1",
        "acquia/blt": "^9.2",
        "drupal/reactjs_mount": "^1.0@alpha",
        "drupal/jsonapi": "^1.23",
        "drupal/jsonapi_extras": "^2.10",
        "drupal/pathauto": "^1.3",
        "drupal/ui_patterns": "^1.0@RC",
        "drupal/google_analytics": "^2.3",
        "drupal/search_api_solr": "^1.2",
        "drupal/components": "^1.0",
        "drupal/redis": "^1.0",
        "drupal/viewsreference": "^1.2",
        "drupal/slick_views": "^1.0@RC",
        "drupal/slick_media": "^1.0",
        "drupal/scheduler": "^1.0",
        "drupal/honeypot": "^1.29",
        "drupal/embed": "^1.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/feeds": "^3.0@alpha",
        "drupal/rules": "^3.0@alpha",
        "drupal/inline_entity_form": "^1.0@RC"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
      "post-install-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ],
      "post-update-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "html/core": [
                "drupal/core"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/contrib/social": [
                "goalgorilla/open_social"
            ],
            "html/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true
    }
}
